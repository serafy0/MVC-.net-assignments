@model List<string>
<label for="CompanyName">Company Name</label>
<select   name="CompanyName"  class="form-control">
    @foreach (var item in Model)
    {

        <option value="@item" selected="@(item == ViewBag.SelectedCompanyName)">@item</option>
    }
</select>
